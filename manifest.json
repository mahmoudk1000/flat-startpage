{
  "manifest_version": 2,
  "name": "start-page-colors",
  "short_name": "start-page-colors",
  "version": "3.1.1",
  "description": "Minimal start page made for elegant look and simple at same time.",
  "author": "mahmoudk1000",
  "icons": {
    "48": "images/icon.png"
  },
  "background": {
    "scripts": [
      "js/time.js",
      "js/weather.js",
      "js/background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "webNavigation"
  ],
  "web_accessible_resources": [
    "index.html"
  ]
}
